# Axiom Assistant Environment Configuration
# Copy this file to .env and configure for your environment

# Logging Configuration
RUST_LOG=info

# Probabilistic Module Configuration
AXIOM_MODEL_PATH=/path/to/your/gguf/model.bin
AXIOM_MAX_TOKENS=2048
AXIOM_TEMPERATURE=0.7

# Deterministic Module Configuration
AXIOM_ENABLE_PROLOG=false
AXIOM_MAX_QUERY_LENGTH=10000

# Security Configuration
# Add any API keys or sensitive configuration here
# Never commit actual .env files to version control
